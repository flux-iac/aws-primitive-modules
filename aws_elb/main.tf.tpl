terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "4.33.0"
    }
  }
}

provider "aws" {
}

variable "values" {
  type = object({
    access_logs = optional(object({
        interval = optional(number)
        bucket = optional(string)
        bucket_prefix = optional(string)
        enabled = optional(bool)
    }))
    connection_draining = optional(bool)
    connection_draining_timeout = optional(number)
    cross_zone_load_balancing = optional(bool)
    desync_mitigation_mode = optional(string)
    idle_timeout = optional(number)
    internal = optional(bool)
    listener = optional(set(any))
    name = optional(string)
    name_prefix = optional(string)
    tags = optional(map(string))
  })
}

resource "aws_elb" "this" {
  access_logs = var.values.access_logs
  connection_draining = var.values.connection_draining
  connection_draining_timeout = var.values.connection_draining_timeout
  cross_zone_load_balancing = var.values.cross_zone_load_balancing
  desync_mitigation_mode = var.values.desync_mitigation_mode
  idle_timeout = var.values.idle_timeout
  internal = var.values.internal
  listener = var.values.listener
  name = var.values.name
  name_prefix = var.values.name_prefix
  tags = var.values.tags
  {{- if $.Values.count }}
  count = {{ $.Values.count }}
  {{- end }}

  {{- if $.Values.lifecycle }}
  lifecycle {
    {{- if $.Values.lifecycle.create_before_destroy }}
    create_before_destroy = {{ $.Values.lifecycle.create_before_destroy }}
    {{- end }}
    {{- if $.Values.lifecycle.ignore_changes }}
    prevent_destroy = {{ $.Values.lifecycle.prevent_destroy }}
    {{- end }}
    {{- if $.Values.lifecycle.ignore_changes }}
    ignore_changes = {{ $.Values.lifecycle.ignore_changes }}
    {{- end }}
  }
  {{- end }}
}


output "arn" {
    value = aws_elb.this.arn
}

output "dns_name" {
    value = aws_elb.this.dns_name
}

output "name" {
    value = aws_elb.this.name
}

output "source_security_group" {
    value = aws_elb.this.source_security_group
}

output "source_security_group_id" {
    value = aws_elb.this.source_security_group_id
}

output "zone_id" {
    value = aws_elb.this.zone_id
}

